{
  "name": "telessh",
  "version": "1.0.0",
  "type": "skill",
  "description": "Telessh - Seeded HyperSSH connection manager with plugin architecture and predictable state management",
  "entry": "skill.js",
  "category": "networking",
  "tags": ["ssh", "hyperssh", "seeded", "connection", "remote", "plugin"],
  "commands": [
    {
      "name": "telessh",
      "description": "Telessh skill with plugin architecture",
      "aliases": ["ssh", "remote"]
    }
  ],
  "requirements": {
    "hyperssh": "^5.0.4"
  },
  "permissions": [
    "network:connect",
    "process:spawn",
    "filesystem:read",
    "filesystem:write"
  ],
  "plugins": {
    "builtin": ["session", "buffer", "logging", "validation"],
    "autoload": true,
    "registry": "https://registry.telessh.io/plugins"
  },
  "config": {
    "maxSessions": 100,
    "maxLogSize": 10000,
    "sessionTimeout": 3600000,
    "autoPersist": true,
    "predictabilityLevel": "strict"
  }
}
